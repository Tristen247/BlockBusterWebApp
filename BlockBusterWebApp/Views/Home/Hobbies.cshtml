@{
    ViewData["Title"] = "Hobbies";
}
<h1>@ViewData["Title"]</h1>

<p>The following list are some of my favorite hobbies</p>

<ul>
    @{
        foreach (var hobbies in ViewBag.Hobbies)
        {
            <li>@hobbies</li>
        }
    }
</ul>
